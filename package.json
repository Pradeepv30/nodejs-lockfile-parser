{
  "name": "@snyk/lockfile-parser",
  "version": "1.0.0",
  "description": "Generate a dep tree given a lockfile",
  "main": "dist/lib/index.js",
  "scripts": {
    "test": "npm run lint && tap --node-arg=--no-deprecation --timeout=150 --jobs=4 $(find tests -name *.test.js) -Rdot --timeout=150",
    "lint": "tslint lib/**/*.ts",
    "build": "tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snyk/lockfile-parser.git"
  },
  "author": "snyk.io",
  "license": "private",
  "homepage": "https://github.com/snyk/lockfile-parser#readme",
  "dependencies": {
    "lodash": "4.17.10",
    "path": "0.12.7"
  },
  "devDependencies": {
    "typescript": "3.0.1",
    "@types/node": "10.5.5"
  }
}
